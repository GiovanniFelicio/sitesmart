<script type="text/javascript">
    google.charts.load("current", {packages:['corechart']});
    google.charts.setOnLoadCallback(drawChart);
    function drawChart() {
      var data = google.visualization.arrayToDataTable([
        ["Element", "Density", { role: "style" } ],
        ["1ª Questão", 10, 'stroke-color: #0077b6; stroke-opacity: 0.8; stroke-width: 4; fill-color: #0077b6; fill-opacity: 0.5'],
        ["2ª Questão", 12, 'stroke-color: #00b4d8; stroke-opacity: 0.8; stroke-width: 4; fill-color: #00b4d8; fill-opacity: 0.5'],
        ["3ª Questão", 16, 'stroke-color: #90e0ef; stroke-opacity: 0.8; stroke-width: 4; fill-color: #90e0ef; fill-opacity: 0.5'],
        ["4ª Questão", 12, 'stroke-color: #75d3ff; stroke-opacity: 0.8; stroke-width: 4; fill-color: #75d3ff; fill-opacity: 0.5'],
        ["5ª Questão", 15, 'stroke-color: #5bbbca; stroke-opacity: 0.8; stroke-width: 4; fill-color: #5bbbca; fill-opacity: 0.5']
      ]);

      var view = new google.visualization.DataView(data);
      view.setColumns([0, 1,
                       { calc: "stringify",
                         sourceColumn: 1,
                         type: "string",
                         role: "annotation" },
                       2]);

      var options = {
        title: "Perguntas - Número de acertos",
        width: 500,
        height: 400,
        bar: {groupWidth: "95%"},
        legend: { position: "none" },
      };
      var chart = new google.visualization.ColumnChart(document.getElementById("chart_div"));
      chart.draw(view, options);
  }
</script>